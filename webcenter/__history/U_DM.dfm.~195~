  object q_financiadoras: TFDQuery
    Connection = banco
    SQL.Strings = (
      'SELECT a.cod_financiadora,'
      '       a.descricao_instituicao,'
      '       decode(a.forma_retorno,'
      '              '#39'1'#39','
      '              '#39'Plano Fixo'#39','
      '              '#39'2'#39','
      '              '#39'Coeficiente'#39','
      '              '#39'3'#39','
      '              '#39'Flex Mixto'#39','
      '              '#39'Indefinido'#39') Forma'
      '  FROM Financiadoras a'
      ' Where ativa = '#39'S'#39
      ' ORDER BY a.DESCRICAO_INSTITUICAO')
    Left = 720
    Top = 272
